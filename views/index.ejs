<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Action Excel</title>
    <link rel="icon" href="public/img/Microsoft_Excel-Logo.png">
    <link rel="stylesheet" href="public/css/style.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
    <script src="https://kit.fontawesome.com/41c9577096.js" crossorigin="anonymous"></script>
</head>

<body>
    <div class="container">
          <section id="fileUploaderInput">
            <h1>File Upload</h1>

            <div class="error">
              <h4 id="error" style="color: red"></h4>
            </div>

              <input type="file" id="inpFile">
              <button type="submit" id="btnUpload">Upload!</button>
              <p id="loading">Loading...</p>
              <p id="process"></p>


          </section>

          <a href="#">
            <button type="button" class="btn btn-outline-primary" data-bs-toggle="modal" data-bs-target="#look-product" style="margin-right: 50px; text-align: center" id="lookBtn">
              Look
            </button>
          </a>
          <section id="action">
            <div class="dropdown-center">
              <button type="button" class="btn btn-warning dropdown-toggle action" data-bs-toggle="dropdown" aria-expanded="false">
                        Action
              </button>
                <ul class="dropdown-menu ul-action">
                      <li><a class="dropdown-item" href="#"><button type="button" class="btn btn-outline-primary" data-bs-toggle="modal" data-bs-target="#products" style="width: 200px; text-align: center" id="productsEditBtn">
                                            products </button></a></li>
                </ul>
            </div>
            <i class="fa-solid fa-exclamation excla-action"></i>
          </section>
    </div>

    <!-- Modal -->
    <div class="modal fade" id="products" tabindex="-1" aria-labelledby="product-edit" aria-hidden="true">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="product-edit">Products</h5>
            <button type="button" class="btn btn-primary ms-5" data-type='add-product' id="addProduct">add</button>
            <button type="button" class="btn btn-outline-primary ms-2" data-bs-toggle="modal" data-bs-target="#cancelled-products" style="width: 150px; text-align: center" id="cancelledProductBtn">
                                  cancelled </button>
            <span class="search">
                <i class="fa-solid fa-magnifying-glass searchBtn"></i>
                <input type="search" placeholder="search...">
            </span>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
          </div>
          <div class="modal-body">

          </div>
          <div class="modal-footer" >
              <div id="pagination-container-product">
              </div>
              <div class="btns">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
              </div>
          </div>
        </div>
      </div>
    </div>

    <!-- Modal -->
    <div class="modal fade" id="cancelled-products" tabindex="-1" aria-labelledby="cancelledProduct" aria-hidden="true">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="btn btn-outline-primary" style="padding: 0; border: none; margin: 5px; margin-right: 12px;" id="backProduct">
            <svg xmlns="http://www.w3.org/2000/svg" width="27" height="27" fill="currentColor" class="bi bi-backspace" viewBox="0 0 16 16">
                <path d="M5.83 5.146a.5.5 0 0 0 0 .708L7.975 8l-2.147 2.146a.5.5 0 0 0 .707.708l2.147-2.147 2.146 2.147a.5.5 0 0 0 .707-.708L9.39 8l2.146-2.146a.5.5 0 0 0-.707-.708L8.683 7.293 6.536 5.146a.5.5 0 0 0-.707 0z"/>
                <path d="M13.683 1a2 2 0 0 1 2 2v10a2 2 0 0 1-2 2h-7.08a2 2 0 0 1-1.519-.698L.241 8.65a1 1 0 0 1 0-1.302L5.084 1.7A2 2 0 0 1 6.603 1h7.08zm-7.08 1a1 1 0 0 0-.76.35L1 8l4.844 5.65a1 1 0 0 0 .759.35h7.08a1 1 0 0 0 1-1V3a1 1 0 0 0-1-1h-7.08z"/>
              </svg></button>
            <h5 class="modal-title" id="cancelledProduct">Cancelled Products</h5>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
          </div>
          <div class="modal-body" id="data-container-cancelled-product">

          </div>
          <div class="modal-footer" id="pagination-container-cancelled-product">

          </div>
        </div>
      </div>
    </div>


    <!-- Modal -->
    <div class="modal fade" id="product-detail" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="product-detail-label" aria-hidden="true">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="product-detail-label">Product Detail</h5>
            <button type="button" class="btn btn-primary" id="addSizeBtn">
              add
            </button>
            <button type="button" class="btn btn-primary" id="isEnabledBtn">
            </button>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
          </div>
          <div class="modal-body" id="product-detail-body">

          </div>
          <div class="modal-footer">
            <div class="alert op-0" role="alert">
            </div>
            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
            <button type="button" class="btn btn-primary backToProductBtn">
              Back
            </button>
          </div>
        </div>
      </div>
    </div>
    <!-- Modal -->
    <div class="modal fade" id="look-product" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="look-product-label" aria-hidden="true">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="product-detail-label">Processed Data</h5>
            <button type="button" class="btn btn-danger" id="deleteBtn">
              Delete
            </button>
            <select class="form-select form-select-sm" aria-label=".form-select-sm" style="
                width: 130px;
                margin: 0 20px;
            ">
              <option selected>Filter</option>
              <option value="1">Active</option>
              <option value="0">Inactive</option>
            </select>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
          </div>
          <div class="modal-body" id="look-product-detail">

          </div>
          <div class="modal-footer">
            <div class="alert op-0" role="alert">
            </div>
            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
          </div>
        </div>
      </div>
    </div>


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous">
    </script>

    <script src="dist/bundle.js"></script>
</body>

</html>
